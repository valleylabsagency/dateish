{
  "expo": {
    "name": "Dateish",
    "slug": "Dateish",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideExponentSplashScreen": false
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#460b2a",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Dateish"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tylerjvalley.Dateish",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.tylerjvalley.Dateish",
      "icon": "./assets/images/icon.png",
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-font",
      "expo-web-browser",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "91d81c7f-935a-4bb4-8f75-062721f369ba"
      }
    }
  }
}
